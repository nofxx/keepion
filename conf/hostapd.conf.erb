interface=wlan0
#driver=nl80211
ssid=<%= @essid || 'KeePion' %>
hw_mode=g
channel=7
#wmm_enabled=1
#macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_passphrase=<%= @password || '12345678' %>
wpa_key_mgmt=WPA-PSK
wpa_pairwise=TKIP CCMP
rsn_pairwise=CCMP
ctrl_interface=/var/run/hostapd
ctrl_interface_group=0
